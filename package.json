{
  "name": "emoo",
  "version": "1.0.0",
  "description": "CLI tool for IMAP email migration and cPanel bulk operations",
  "type": "module",
  "main": "index.js",
  "bin": {
    "emoo": "./bin/run.js"
  },
  "scripts": {
    "build": "echo 'No build needed for JS'",
    "start": "./bin/run.js",
    "dev": "./bin/dev.js",
    "test": "node bin/run.js --help",
    "prepublishOnly": "npm run test",
    "release": "release-it"
  },
  "keywords": [
    "imap",
    "email",
    "migration",
    "cpanel",
    "bulk",
    "cli"
  ],
  "author": {
    "name": "Farhan Brillan Widyakirana",
    "email": "fbrillanw@gmail.com",
    "url": "https://github.com/k1rana"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/k1rana/emoo.git"
  },
  "bugs": {
    "url": "https://github.com/k1rana/emoo/issues"
  },
  "homepage": "https://github.com/k1rana/emoo#readme",
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.8.0",
  "dependencies": {
    "@oclif/core": "^4.5.2",
    "@oclif/plugin-help": "^6.2.32",
    "axios": "^1.11.0",
    "chalk": "^5.6.0",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "fs-extra": "^11.3.1",
    "inquirer": "^12.9.4",
    "listr2": "^9.0.2",
    "ora": "^8.2.0"
  },
  "oclif": {
    "bin": "emoo",
    "commands": "./src/commands",
    "dirname": "emoo",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "devDependencies": {
    "@types/node": "^18",
    "release-it": "^19.0.4",
    "ts-node": "^10.9.2"
  }
}
